{
    "compilerOptions": {
      "target": "ES5",
      "outDir": "dist",
      "module": "commonjs",
      "strict": true,
      "sourceMap": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "types": [
        "node"
      ]
    },
    "exclude": [
      "node_modules",
      "dist"
    ],
    "include": [
      "*.ts",
      "src/**/*.ts",
      "enviroments.d.ts",
      "launcher.ts"
    ]
  }